    "@types/jest": "29.5.12",
    "@types/uuid": "10.0.0",
    "@types/node": "20.14.11",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "babel-jest": "29.7.0",
    "eslint": "8.57.0",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "supertest": "7.0.0",
    "typescript": "5.5.4"
